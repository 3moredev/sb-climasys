# Climasys Database Migration Configuration
# Copy this file to .env and update with your actual values

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=climasys_dev
DB_USER=postgres
DB_PASSWORD=root

# CSV Data Directory
CSV_DIRECTORY=./csv_data

# Optional: SQL Server Configuration (for reference)
# SQLSERVER_HOST=your_sqlserver_host
# SQLSERVER_PORT=1433
# SQLSERVER_DATABASE=Climasys-00010
# SQLSERVER_USER=your_sqlserver_user
# SQLSERVER_PASSWORD=your_sqlserver_password

# Migration Options
# SKIP_SCHEMA=false
# SKIP_REFERENCE=false
# SKIP_MIGRATION=false
# SKIP_CSV=false

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=migration.log
