--
-- TOC entry 486 (class 1255 OID 33692)
-- Name: splitfunction(character varying, character); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.splitfunction(input_string character varying, delimiter character) RETURNS TABLE(id integer, string text)
    LANGUAGE sql
    AS $$
WITH RECURSIVE split(stpos, endpos) AS (
    SELECT 1, POSITION(delimiter IN input_string)
    UNION ALL
    SELECT endpos + 1, POSITION(delimiter IN SUBSTRING(input_string FROM endpos + 1)) + endpos
    FROM split
    WHERE endpos > 0
)
SELECT ROW_NUMBER() OVER (ORDER BY stpos) AS id,
       SUBSTRING(input_string FROM stpos FOR 
                 COALESCE(NULLIF(endpos,0), LENGTH(input_string)+1) - stpos) AS string
FROM split;
$$;


ALTER FUNCTION public.splitfunction(input_string character varying, delimiter character) OWNER TO postgres;

